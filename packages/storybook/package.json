{
  "name": "@lerna-next/storybook",
  "version": "0.0.1",
  "description": "Storybook for UI Kit",
  "keywords": [
    "react",
    "storybook"
  ],
  "author": "Vyacheslav Khanilo <khaniloslava@yandex.ru>",
  "license": "ISC",
  "main": "lib/storybook.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "react-scripts test",
    "start": "start-storybook -p 9009 -s public -c .storybook",
    "build": "build-storybook -s public -c .storybook -o build",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280 --build_dir=build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@lerna-next/ui-kit": "0.0.1",
    "@storybook/addon-knobs": "^5.1.1",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.5",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/storybook__addon-knobs": "^5.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.5.1"
  },
  "devDependencies": {
    "@percy/storybook": "^3.0.2",
    "@storybook/addon-actions": "^5.1.1",
    "@storybook/addon-info": "^5.1.1",
    "@storybook/addon-links": "^5.1.1",
    "@storybook/addon-storyshots": "^5.1.1",
    "@storybook/addons": "^5.1.1",
    "@storybook/react": "^5.1.1",
    "@types/storybook__react": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "8.0.5",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-test-renderer": "^16.8.6",
    "require-context.macro": "^1.0.4"
  }
}
